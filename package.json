{
  "name": "access-code-management",
  "version": "1.0.0",
  "private": true,
  "description": "Gallup Access - Access Code Management",
  "main": "index.js",
  "scripts": {
    "dev:build:server": "webpack --config webpack.server.js",
    "test": "./node_modules/.bin/jest --watchAll",
    "start": "webpack-dev-server --debug --devtool cheap-module-source-map --output-pathinfo --history-api-fallback --hot --inline --progress --colors --open",
    "build": "webpack -p --progress --colors"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-test))"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
